{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["airbnb/base"],
    "rules": {
        "linebreak-style": 0,
        "prefer-destructuring": ["error", {"object": true, "array": false}],
        "no-param-reassign": "off",
        "func-names": "off"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
}
